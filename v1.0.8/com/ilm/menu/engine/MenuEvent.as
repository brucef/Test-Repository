package com.ilm.menu.engine{	import flash.events.Event;	public class MenuEvent extends Event	{		public static const MENU_CLOSED:String = "menu closed";		public var arg:*;		public function MenuEvent(type:String, customArg:* = null, bubbles:Boolean = false, cancelable:Boolean = false)		{			super(type, bubbles, cancelable);			this.arg = customArg;		}		public override function clone():Event		{			return new MenuEvent(type, arg, bubbles, cancelable);		}		public override function toString():String		{			return formatToString("MenuEvent", "type", "arg", "bubbles", "cancelable", "eventPhase");		}	}}