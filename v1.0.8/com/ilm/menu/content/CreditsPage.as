package com.ilm.menu.content{	import flash.events.Event;	import com.ilm.menu.engine.BaseMenu;	import com.ilm.utils.display.*;	import com.ilm.utils.buttons.ButtonCore;	import com.ilm.menu.Menu;		public class CreditsPage extends BaseMenu	{		private var menuClass:Menu;		public function CreditsPage(passedClass:Menu):void		{			menuClass = passedClass;			this.addEventListener(Event.ADDED_TO_STAGE, init);		}		private function init(event:Event):void		{			var _draw:Draw = new Draw();			_draw.Rectange(this,0,0,stage.stageWidth,stage.stageHeight,0x6699FF);			var _text:Field = new Field();			_text.define("Credits Page",50,50,"Myriad Pro");			this.addChild(_text);			this.openMe(this);			makeButtons();		}		private function makeButtons():void		{			var _draw:Draw = new Draw();			var _main:ButtonCore = new ButtonCore(ButtonCore.LIGHT_DARK, toMain);			_draw.Rectange(_main,0,0,100,20,0x00FF99)			_main.x = 250;			_main.y = 150;			this.addChild(_main);		}		private function toMain(event:Event):void		{			trace("return clicked");			menuClass.openMainPage();			this.closeMe(this);		}	}}